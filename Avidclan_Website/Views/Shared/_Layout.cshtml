    <script type="text/javascript">
        $(document).ready(function () {
            var locationForm = $("#location_form");
            $("#phone").intlTelInput({
                utilsScript: "/assets/js/utils.js",
                separateDialCode: true,
                initialCountry: "us",

            });

            $(".numberonly").keypress(function (e) {
                var charCode = e.which ? e.which : event.keyCode;

                if (String.fromCharCode(charCode).match(/[^0-9]/g)) {
                    return false;
                }
            });



            locationForm.validate({
                errorPlacement: function (error, element) {

                    if (element.attr("id") === "phone") {

                        element.closest(".intl-tel-input").after(error);
                    } else {

                        error.insertAfter(element);
                    }
                },
                rules: {
                    firstname: "required",

                    email: {
                        required: true,
                        email: true,
                    },
                    phone: {
                        required: true,
                        digits: true,
                        minlength: 7,
                        maxlength: 15

                    },
                    message: { // Matches name="message"
                        required: true,
                        minlength: 10, // Reduced from 25 for broader usability, adjust as needed
                        maxlength: 500 // Matches maxlength attribute in textarea
                    }
                    // fcaptcha: { // Your HTML doesn't have a 'fcaptcha' field for this form
                    //     required: true,
                    // },
                },
                messages: {
                    firstname: "Please enter your Full Name",
                    // lastname: "Please enter your Last Name",
                    phone: {
                        required: "Please enter phone number",
                        digits: "Please enter valid phone number (digits only)",
                        minlength: "Please enter a valid phone number",
                        maxlength: "Please enter a valid phone number",
                    },
                    email: {
                        required: "Please enter email address",
                        email: "Please enter a valid email address.",
                    },
                    message: {
                        required: "Please write your Message",
                        minlength: "Please write at least 10 characters",
                    }
                },
                submitHandler: function (form) {
                    $('#btnSubmit').prop("disabled", true);
                    $('#button-spinner').show();
                    form.submit();
                }
            });


            // --- Form Submission Handler (AJAX) ---
            locationForm.submit(function (e) {
                e.preventDefault(); // Prevent the default form submission

                // Re-check validity (belt and suspenders approach)
                if (!locationForm.valid()) {
                    // Re-enable button if validation fails here before AJAX attempt
                    $('#btnSubmit').prop("disabled", false);
                    $('#button-spinner').hide();
                    return false;
                }

                // Show spinner and disable button
                $('#button-spinner').show();
                $('#btnSubmit').prop("disabled", true); // Use the correct button ID from your HTML: btnSubmit

                // Get country code from intlTelInput
                var countryCode = "";
                try {
                    // Ensure intlTelInput instance exists before calling methods
                    if ($("#phone").data('plugin_intlTelInput')) {
                        countryCode = $("#phone").intlTelInput("getSelectedCountryData").dialCode;
                    } else {
                        console.warn("intlTelInput not initialized on #phone");
                    }
                } catch (error) {
                    console.error("Error getting country code:", error);
                    // Decide how to handle this - maybe default code or prevent submission?
                }


                // Update hidden field value (Make sure the ID matches your HTML)
                $("#CountryCode").val(countryCode);
                var dataToSend = {
                    FirstName: $("#firstname").val(),
                    // LastName: $("#lastname").val(), // Remove if no lastname field exists
                    Email: $("#email").val(),
                    PhoneNumber: $("#phone").val(), // Changed name slightly for clarity
                    Message: $("#message").val(),
                    CountryCode: countryCode, // Send the obtained country code
                };

                // AJAX call
                $.ajax({
                    type: "POST",
                    url: "/api/Mail/SendLocationPageDetails", // Your API endpoint
                    contentType: 'application/json', // Sending JSON data
                    data: JSON.stringify(dataToSend), // Convert JS object to JSON string
                })
                    .done(function (response) {
                        // SUCCESS function
                        console.log("Server Response:", response);
                        window.location.replace("/location-thank-you");

                        locationForm.trigger("reset");

                    })
                    .fail(function (jqXHR, textStatus, errorThrown) {
                        // ERROR function
                        console.error("AJAX Error:", textStatus, errorThrown);

                        alert("There was an error sending your message. Please try again.");
                        // Re-enable the button on failure
                        $('#btnSubmit').prop("disabled", false);

                    })
                    .always(function () {
                        $('#button-spinner').hide();

                    });
            });

        });
    </script>


    @RenderSection("scripts", required: false)

</body>
</html>

