<script>
    function showLocation(event, id) {
        document.querySelectorAll('.location-tabs button').forEach(btn => btn.classList.remove('active'));
        event.currentTarget.classList.add('active');
        document.querySelectorAll('.location-content').forEach(div => div.classList.remove('active'));
        document.getElementById(id).classList.add('active');
    }
</script>

