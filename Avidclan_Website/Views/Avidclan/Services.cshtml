<section class="project-inquiry" id="project_inquiry_form">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <div class="avidclan_section_title text-center mb-50">
                    <h5 class="avidclan_sub_title primary-color">READY TO START A PROJECT?</h5>
                    <div class="sec-title"><h2>To start, use form below to tell us about you and the project</h2></div>
                </div>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-12 col-lg-10">
                <form method="post"
                      class="contact-form project_form"
                      name="project_form_data">
                    <div class="row">
                        <div class="col-md-4 form-group">
                            <label for="firstname">First Name</label>
                            <input type="text" name="firstname" id="firstname" placeholder="First Name" />
                        </div>

                        <div class="col-md-4 form-group">
                            <label for="lastname">Last Name</label>
                            <input type="text" name="lastname" id="lastname" placeholder="Last Name" />
                        </div>

                        <div class="col-md-4 form-group">
                            <label for="email">Email</label>
                            <input type="email" name="email" id="email" placeholder="Your Email Address" />
                        </div>

                        <div class="col-md-4 form-group">
                            <label for="phone">Phone</label>
                            <input id="scode" value="scode" type="hidden" name="scode" />
                            <input type="tel" name="phone" id="phone" class="numberonly" />
                        </div>

                        <div class="col-md-4 form-group">
                            <label for="servicename">Select a Service</label>
                            <select class="selectpicker" name="servicename" id="servicename">
                                <option value="">Select</option>
                                <option value="UI/UX Design">UI/UX Design</option>
                                <option value="Web Development">Web Development</option>
                                <option value="Mobile Application Development">Mobile Application Development</option>
                                <option value="Sharepoint Consulting">Sharepoint Consulting</option>
                                <option value="Microsoft Dynamics CRM">Microsoft Dynamics CRM</option>
                            </select>
                        </div>

                        <div class="col-md-4 form-group">
                            <label for="budget">Budget</label>
                            <select class="selectpicker" name="budget" id="budget">
                                <option value="">Select</option>
                                <option value="Less than one Thousand">< $1K</option>
                                <option value="In between one to five Thousand">$1K - $5K</option>
                                <option value="In between five to ten Thousand">$5K - $10K</option>
                                <option value="More than Ten Thousand">> $10K</option>
                            </select>
                        </div>

                        <div class="col-md-4 form-group">
                            <label for="starttime">How soon you want to start?</label>
                            <select class="selectpicker" name="starttime" id="starttime">
                                <option value="">Select</option>
                                <option value="Right Now">Right now</option>
                                <option value="In few Weeks">In few weeks</option>
                                <option value="In few months">In few months</option>
                            </select>
                        </div>

                        <div class="col-md-4 form-group">
                            <label for="prequirement">Requirement</label>
                            <select class="selectpicker" name="prequirement" id="prequirement">
                                <option value="">Select</option>
                                <option value="Hire Dedicated Team">Hire Dedicated Team</option>
                                <option value="New Project">New Project</option>
                                <option value="Existing Project">Existing Project</option>
                                <option value="Support & Maintenance">Support & Maintenance</option>
                            </select>
                        </div>

                        <div class="col-md-4 form-group">
                            <label for="projectbrief">Brief about the project</label>
                            <textarea name="projectbrief" placeholder="" rows="10" id="projectbrief"></textarea>
                        </div>

                        <div class="col-md-4 form-group">
                            <label>Please verify you are a human</label>
                            @Html.Recaptcha()
                            @*<div class="g-recaptcha" data-sitekey="6Lc7VFgjAAAAAJQuEw_pFI_HgbdU6SVsO5uc4QIt"></div>*@
                        </div>

                        <div class="col-md-6 form-group my-auto text-end">
                            <button class="theme-btn btn-style-one" type="submit" name="submit">
                                <span class="btn-title">
                                    Let's Talk
                                    <div id="button-spinner" style="margin-left: 10px; width: 15px; height: 15px; display: none" class="spinner-border"></div>
                                </span>
                            </button>
                        </div>
                    </div>
                    <!-- form message -->
                    <div class="row">
                        <div class="col-12">
                            <div class="alert alert-success project_msg mt-2 mb-0" style="display: none" role="alert"></div>
                        </div>
                    </div>
                    <!-- end message -->
                </form>
            </div>
        </div>
    </div>
</section>

@Html.Partial("_Footer")

